public class NumberSeries {
    public static void generateSeries(int x) {
        for (int i = 1; i <= x; i++) {
            System.out.print(i);
            if (i != x) {
                System.out.print(", ");
            }
            i++;
        }
    }

    public static void main(String[] args) {
        int x = 100;
        generateSeries(x);
    }
}
